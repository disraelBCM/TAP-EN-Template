}



@functions {
    public string AdjustURL(string strURL, bool appendTracking = true)
    {
        if (string.IsNullOrEmpty(strURL))
            return strURL;

        // Ensure absolute URL
        if (!(strURL.StartsWith("https://") || strURL.StartsWith("http://")))
        {
            strURL = "https://www.ascopost.com" + strURL;
        }

        // 1️⃣ Append mode from current page if present
        var mode = HttpContext.Current.Request.QueryString["mode"];
        if (!string.IsNullOrEmpty(mode))
        {
            strURL += strURL.Contains("?") ? "&mode=" + mode : "?mode=" + mode;
        }

        // 2️⃣ Append tracking parameters if requested
        if (appendTracking)
        {
            string token = "{" + "{lead.MD5}" + "}"; // safe for Razor parsing
            string tracking = "utm_source=ONC-TAP-EN-${utm-source}&utm_medium=email&md5=" + token;
            strURL += strURL.Contains("?") ? "&" + tracking : "?" + tracking;
        }

        return strURL;
    }
}



